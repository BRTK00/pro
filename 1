#include <iostream>
#include <cstdlib>
#include <ctime>

using namespace std;

int main() {
  int tab[100];
  int x;

  srand(time(NULL));

  for (int i = 0; i < 100; i++) {
    tab[i] = rand() % 1001;
  }

  cout << "Wprowadz liczbe calkowita: ";
  cin >> x;

  cout << "Elementy tablicy wieksze od " << x << " to:" << endl;
  for (int i = 0; i < 100; i++) {
    if (tab[i] > x) {
      cout << tab[i] << endl;
    }
  }

  return 0;
}
