#include <iostream>
#include <fstream>

using namespace std;

int main() {
  int n;
  string zdanie = "ala ma kota";
  ofstream plik;

  cout << "Podaj liczbe n: ";
  cin >> n;

  plik.open("plik.txt");

  for (int i = 0; i < n; i++) {
    plik << zdanie << endl;
  }

  plik.close();

  return 0;
}
